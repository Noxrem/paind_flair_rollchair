#include "blinky_tinyK22.h"
#include "pin_mux.h"
#include "MK22F51212.h"
#include "fsl_gpio.h"
#include "delay.h"

void blinky(void)
{
	while(1)
	{
		GPIO_PortToggle(BOARD_INITPINS_LED_BLUE_GPIO, BOARD_INITPINS_LED_BLUE_PIN_MASK);
		delay(1000);
	}
}
