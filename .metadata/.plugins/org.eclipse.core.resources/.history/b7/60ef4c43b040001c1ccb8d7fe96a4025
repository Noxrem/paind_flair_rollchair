# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: PAIND_tinyK22_flair.axf

# Tool invocations
PAIND_tinyK22_flair.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map="PAIND_tinyK22_flair.map" -Xlinker --gc-sections -Xlinker -print-memory-usage -Xlinker --sort-section=alignment -Xlinker --cref -mcpu=cortex-m4 -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb -T Blinky_tinyK22_Debug.ld -o "PAIND_tinyK22_flair.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(EXECUTABLES)$(OBJS)$(C_DEPS) PAIND_tinyK22_flair.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-objcopy -v -O binary "PAIND_tinyK22_flair.axf" "PAIND_tinyK22_flair.bin" ; arm-none-eabi-objcopy -v -O srec "PAIND_tinyK22_flair.axf" "PAIND_tinyK22_flair.s19" ; arm-none-eabi-objcopy -v -O ihex "PAIND_tinyK22_flair.axf" "PAIND_tinyK22_flair.hex" ; arm-none-eabi-size "PAIND_tinyK22_flair.axf" ; # arm-none-eabi-objcopy -v -O binary "PAIND_tinyK22_flair.axf" "PAIND_tinyK22_flair.bin" ; # checksum -p MK22FN512xxx12 -d "PAIND_tinyK22_flair.bin"
	-@echo ' '

